 <style>
        .product {
            border: 1px solid #ccc;
            padding: 10px;
            margin-bottom: 10px;
        }
        .product img {
            max-width: 100px;
            max-height: 100px;
        }
    </style>
<div>
  <h1>Lista de Productos</h1>
    <div id="productList">
        {{#each productos}}
            <div class="product">
                <h2>{{title}}</h2>
                <p>{{description}}</p>
                <p>Código: {{code}}</p>
                <p>Precio: ${{price}}</p>
                <p>Stock: {{stock}}</p>
                <p>Categoría: {{category}}</p>
                <div>
                    {{#each thumbnails}}
                        <img src="{{this}}" alt="Thumbnail">
                    {{/each}}
                </div>
            </div>
        {{/each}}
    </div>
</div>

